<h1>Authenticate</h1>
<div class="row">
<div class="col-md-6 col-md-offset-3">
<p>Please enter the code that was sent to your email to finish logging in.</p>
<%= form_for(:session, url: finish_login_path) do |f| %>
  <%= f.label :code %>
  <%= f.text_field :code, class: 'form_control' %>
  <br/>
  <%= f.submit 'Enter', class: 'btn btn-primary' %>
<% end %>
</div>
</div>